{
  "source": [
    "tokens_src/jsonTokens/themes/Saraiva/**/*.json"
  ],
  "platforms": {
    "compose": {
      "transforms": [
        "attribute/cti",
        "name/cti/snake",
        "name/composeColorName",
        "name/composeColorReference",
        "name/composeElevation",
        "name/composeFontFamily",
        "name/composeFontWeight",
        "size/px_to_dp",
        "size/px_to_sp"
      ],
      "transformGroup": "compose",
      "buildPath": "pegasusdesignsystemandroid/src/main/java/br/com/cogna/pegasusdesignsystemandroid/brands/saraiva/tokens/",
      "files": [
        {
          "destination": "SaraivaPalette.kt",
          "format": "compose/object",
          "className": "SaraivaPalette",
          "packageName": "br.com.cogna.pegasusdesignsystemandroid.brands.saraiva.tokens",
          "filter": {
            "attributes": {
              "category": "palette"
            }
          }
        },

        {
          "destination": "SaraivaSchemes.kt",
          "format": "compose/object",
          "className": "SaraivaSchemes",
          "packageName": "br.com.cogna.pegasusdesignsystemandroid.brands.saraiva.tokens",
          "filter": {
            "attributes": {
              "type": "color"
            }
          }
        },

        {
          "destination": "SaraivaSpacing.kt",
          "format": "compose/object",
          "className": "SaraivaSpacing",
          "packageName": "br.com.cogna.pegasusdesignsystemandroid.brands.saraiva.tokens",
          "type": "float",
          "filter": {
            "attributes": {
              "category": "spacing"
            }
          }
        },

        {
          "destination": "SaraivaBorders.kt",
          "format": "compose/object",
          "className": "SaraivaBorders",
          "packageName": "br.com.cogna.pegasusdesignsystemandroid.brands.saraiva.tokens",
          "type": "float",
          "filter": {
            "attributes": {
              "category": "border"
            }
          }
        },

        {
          "destination": "SaraivaElevation.kt",
          "format": "compose/object",
          "className": "SaraivaElevation",
          "packageName": "br.com.cogna.pegasusdesignsystemandroid.brands.saraiva.tokens",
          "filter": {
            "attributes": {
              "category": "elevation"
            }
          }
        },

        {
          "destination": "SaraivaFontSizes.kt",
          "format": "compose/object",
          "className": "SaraivaFontSizes",
          "packageName": "br.com.cogna.pegasusdesignsystemandroid.brands.saraiva.tokens",
          "filter": {
            "attributes": {
              "category": "fontSize"
            }
          }
        },

        {
          "destination": "SaraivaLineHeight.kt",
          "format": "compose/object",
          "className": "SaraivaLineHeight",
          "packageName": "br.com.cogna.pegasusdesignsystemandroid.brands.saraiva.tokens",
          "filter": {
            "attributes": {
              "category": "lineHeight"
            }
          }
        },

        {
          "destination": "SaraivaFontFamily.kt",
          "format": "compose/object",
          "className": "SaraivaFontFamily",
          "packageName": "br.com.cogna.pegasusdesignsystemandroid.brands.saraiva.tokens",
          "options": {
            "outputReferences": true
          },
          "filter": {
            "attributes": {
              "category": "fontFamily"
            }
          }
        },

        {
          "destination": "SaraivaFontWeight.kt",
          "format": "compose/object",
          "className": "SaraivaFontWeight",
          "packageName": "br.com.cogna.pegasusdesignsystemandroid.brands.saraiva.tokens",
          "type": "float",
          "options": {
            "outputReferences": true
          },
          "filter": {
            "attributes": {
              "category": "fontWeight"
            }
          }
        },

        {
          "destination": "SaraivaTypography.kt",
          "format": "compose/typography",
          "className": "SaraivaTypography",
          "packageName": "br.com.cogna.pegasusdesignsystemandroid.brands.saraiva.tokens",
          "options": {
            "outputReferences": true
          },
          "filter": {
            "attributes": {
              "category": "typography"
            }
          }
        }
      ]
    },
    "android": {
      "transforms": [
        "attribute/cti",
        "name/cti/snake",
        "color/hex8android",
        "size/sp",
        "size/dp"
      ],
      "transformGroup": "android",
      "buildPath": "pegasusdesignsystemandroid/src/main/res/values/",
      "files": [
        {
          "destination": "saraiva_tokens_dimens.xml",
          "format": "android/fontDimens"
        },
        {
          "destination": "saraiva_tokens_colors.xml",
          "format": "android/colors"
        }
      ],
      "options": {
        "showFileHeader": false
      }
    }
  }
}

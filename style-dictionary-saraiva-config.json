{
  "source": [
    "tokens_src/jsonTokens/themes/Saraiva/**/*.json"
  ],
  "platforms": {
    "compose": {
      "transforms": [
        "attribute/cti",
        "name/cti/snake",
        "name/composeColorName",
        "name/composeColorReference",
        "name/composeElevation",
        "name/composeFontFamily",
        "name/composeFontWeight",
        "size/px_to_dp",
        "size/px_to_sp",
        "size/composeLineHeight"
      ],
      "transformGroup": "compose",
      "buildPath": "pegasusdesignsystemandroid/src/main/java/br/com/cogna/pegasusdesignsystemandroid/brands/saraiva/tokens/",
      "files": [
        {
          "destination": "SaraivaPaletteTokens.kt",
          "format": "compose/object",
          "className": "SaraivaPaletteTokens",
          "packageName": "br.com.cogna.pegasusdesignsystemandroid.brands.saraiva.tokens",
          "filter": {
            "attributes": {
              "category": "palette"
            }
          }
        },

        {
          "destination": "SaraivaColorSchemesTokens.kt",
          "format": "compose/object",
          "className": "SaraivaColorSchemesTokens",
          "packageName": "br.com.cogna.pegasusdesignsystemandroid.brands.saraiva.tokens",
          "filter": {
            "attributes": {
              "type": "color"
            }
          }
        },

        {
          "destination": "SaraivaSpacingTokens.kt",
          "format": "compose/object",
          "className": "SaraivaSpacingTokens",
          "packageName": "br.com.cogna.pegasusdesignsystemandroid.brands.saraiva.tokens",
          "type": "float",
          "filter": {
            "attributes": {
              "category": "spacing"
            }
          }
        },

        {
          "destination": "SaraivaBorderTokens.kt",
          "format": "compose/object",
          "className": "SaraivaBorderTokens",
          "packageName": "br.com.cogna.pegasusdesignsystemandroid.brands.saraiva.tokens",
          "type": "float",
          "filter": {
            "attributes": {
              "category": "border"
            }
          }
        },

        {
          "destination": "SaraivaElevationTokens.kt",
          "format": "compose/object",
          "className": "SaraivaElevationTokens",
          "packageName": "br.com.cogna.pegasusdesignsystemandroid.brands.saraiva.tokens",
          "filter": {
            "attributes": {
              "category": "elevation"
            }
          }
        },

        {
          "destination": "SaraivaFontSizeTokens.kt",
          "format": "compose/object",
          "className": "SaraivaFontSizeTokens",
          "packageName": "br.com.cogna.pegasusdesignsystemandroid.brands.saraiva.tokens",
          "filter": {
            "attributes": {
              "category": "fontSize"
            }
          }
        },

        {
          "destination": "SaraivaLineHeightTokens.kt",
          "format": "compose/object",
          "className": "SaraivaLineHeightTokens",
          "packageName": "br.com.cogna.pegasusdesignsystemandroid.brands.saraiva.tokens",
          "filter": {
            "attributes": {
              "category": "lineHeight"
            }
          }
        },

        {
          "destination": "SaraivaFontFamilyTokens.kt",
          "format": "compose/object",
          "className": "SaraivaFontFamilyTokens",
          "packageName": "br.com.cogna.pegasusdesignsystemandroid.brands.saraiva.tokens",
          "options": {
            "outputReferences": true
          },
          "filter": {
            "attributes": {
              "category": "fontFamily"
            }
          }
        },

        {
          "destination": "SaraivaFontWeightTokens.kt",
          "format": "compose/object",
          "className": "SaraivaFontWeightTokens",
          "packageName": "br.com.cogna.pegasusdesignsystemandroid.brands.saraiva.tokens",
          "type": "float",
          "options": {
            "outputReferences": true
          },
          "filter": {
            "attributes": {
              "category": "fontWeight"
            }
          }
        },

        {
          "destination": "SaraivaTypographyTokens.kt",
          "format": "compose/typography",
          "className": "SaraivaTypographyTokens",
          "packageName": "br.com.cogna.pegasusdesignsystemandroid.brands.saraiva.tokens",
          "options": {
            "outputReferences": true
          },
          "filter": {
            "attributes": {
              "category": "typography"
            }
          }
        }
      ]
    },
    "android": {
      "transforms": [
        "attribute/cti",
        "name/cti/snake",
        "color/hex8android",
        "size/sp",
        "size/dp"
      ],
      "transformGroup": "android",
      "buildPath": "pegasusdesignsystemandroid/src/main/res/values/",
      "files": [
        {
          "destination": "saraiva_tokens_dimens.xml",
          "format": "android/fontDimens"
        },
        {
          "destination": "saraiva_tokens_colors.xml",
          "format": "android/colors"
        }
      ],
      "options": {
        "showFileHeader": false
      }
    }
  }
}

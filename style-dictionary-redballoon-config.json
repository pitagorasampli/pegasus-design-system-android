{
  "source": [
    "tokens_src/jsonTokens/themes/Red Balloon/**/*.json"
  ],
  "platforms": {
    "compose": {
      "transforms": [
        "attribute/cti",
        "name/cti/snake",
        "name/composeColorName",
        "name/composeColorReference",
        "name/composeFontFamily",
        "name/composeFontWeight",
        "size/px_to_dp",
        "size/px_to_sp",
        "size/composeLineHeight",
        "shadow/compose"
      ],
      "transformGroup": "compose",
      "buildPath": "pegasusdesignsystemandroid/src/main/java/br/com/cogna/pegasusdesignsystemandroid/brands/redBalloon/tokens/",
      "files": [
        {
          "destination": "RedBalloonPaletteTokens.kt",
          "format": "compose/object",
          "className": "RedBalloonPaletteTokens",
          "packageName": "br.com.cogna.pegasusdesignsystemandroid.brands.redBalloon.tokens",
          "filter": {
            "attributes": {
              "category": "palette"
            }
          }
        },

        {
          "destination": "RedBalloonColorSchemesTokens.kt",
          "format": "compose/object",
          "className": "RedBalloonColorSchemesTokens",
          "packageName": "br.com.cogna.pegasusdesignsystemandroid.brands.redBalloon.tokens",
          "filter": {
            "attributes": {
              "type": "color"
            }
          }
        },

        {
          "destination": "RedBalloonSpacingTokens.kt",
          "format": "compose/object",
          "className": "RedBalloonSpacingTokens",
          "packageName": "br.com.cogna.pegasusdesignsystemandroid.brands.redBalloon.tokens",
          "type": "float",
          "filter": {
            "attributes": {
              "category": "spacing"
            }
          }
        },

        {
          "destination": "RedBalloonBorderTokens.kt",
          "format": "compose/object",
          "className": "RedBalloonBorderTokens",
          "packageName": "br.com.cogna.pegasusdesignsystemandroid.brands.redBalloon.tokens",
          "type": "float",
          "filter": {
            "attributes": {
              "category": "border"
            }
          }
        },

        {
          "destination": "RedBalloonFontSizeTokens.kt",
          "format": "compose/object",
          "className": "RedBalloonFontSizeTokens",
          "packageName": "br.com.cogna.pegasusdesignsystemandroid.brands.redBalloon.tokens",
          "filter": {
            "attributes": {
              "category": "fontSize"
            }
          }
        },

        {
          "destination": "RedBalloonLineHeightTokens.kt",
          "format": "compose/object",
          "className": "RedBalloonLineHeightTokens",
          "packageName": "br.com.cogna.pegasusdesignsystemandroid.brands.redBalloon.tokens",
          "filter": {
            "attributes": {
              "category": "lineHeight"
            }
          }
        },

        {
          "destination": "RedBalloonFontFamilyTokens.kt",
          "format": "compose/object",
          "className": "RedBalloonFontFamilyTokens",
          "packageName": "br.com.cogna.pegasusdesignsystemandroid.brands.redBalloon.tokens",
          "options": {
            "outputReferences": true
          },
          "filter": {
            "attributes": {
              "category": "fontFamily"
            }
          }
        },

        {
          "destination": "RedBalloonFontWeightTokens.kt",
          "format": "compose/object",
          "className": "RedBalloonFontWeightTokens",
          "packageName": "br.com.cogna.pegasusdesignsystemandroid.brands.redBalloon.tokens",
          "type": "float",
          "options": {
            "outputReferences": true
          },
          "filter": {
            "attributes": {
              "category": "fontWeight"
            }
          }
        },

        {
          "destination": "RedBalloonTypographyTokens.kt",
          "format": "compose/typography",
          "className": "RedBalloonTypographyTokens",
          "packageName": "br.com.cogna.pegasusdesignsystemandroid.brands.redBalloon.tokens",
          "options": {
            "outputReferences": true
          },
          "filter": {
            "attributes": {
              "category": "typography"
            }
          }
        },
        {
          "destination": "RedBalloonShadowTokens.kt",
          "format": "compose/object",
          "className": "RedBalloonShadowTokens",
          "packageName": "br.com.cogna.pegasusdesignsystemandroid.brands.redBalloon.tokens",
          "options": {
            "outputReferences": true
          },
          "filter": {
            "attributes": {
              "category": "shadow"
            }
          }
        }
      ]
    },
    "android": {
      "transforms": [
        "attribute/cti",
        "name/cti/snake",
        "color/hex8android",
        "size/sp",
        "size/dp"
      ],
      "transformGroup": "android",
      "buildPath": "pegasusdesignsystemandroid/src/main/res/values/",
      "files": [
        {
          "destination": "redBalloon_tokens_dimens.xml",
          "format": "android/fontDimens"
        },
        {
          "destination": "redBalloon_tokens_colors.xml",
          "format": "android/colors"
        }
      ],
      "options": {
        "showFileHeader": false
      }
    }
  }
}
